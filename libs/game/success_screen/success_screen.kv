#:kivy 1.8.0
<SuccessScreen>:
    orientation: 'vertical'
    
    Label:
        id: score
        text: 'Score: '

    BoxLayout:
        orientation: 'horizontal'
        size_hint_y: .2

        Button:
            text: 'Retry'
            on_press: root.retry_callback() 

        Button:
            text: 'Next Level'
            on_press: root.next_callback() 


